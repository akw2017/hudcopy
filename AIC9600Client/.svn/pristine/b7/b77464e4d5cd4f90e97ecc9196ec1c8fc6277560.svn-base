using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CitizenSoftwareLib.Network.WCF
{
    public class WCFTimeout
    {
        public int CloseTimeoutSeconds { get; set; }
        public int OpenTimeoutSeconds { get; set; }
        public int ReceiveTimeoutSeconds { get; set; }
        public int SendTimeoutSeconds { get; set; }

        public WCFTimeout()
        {
            CloseTimeoutSeconds = 10;
            OpenTimeoutSeconds = 5;
            ReceiveTimeoutSeconds = 30;
            SendTimeoutSeconds = 30;
        }
    }
}
