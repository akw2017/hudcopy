            </DataTrigger>
            <DataTrigger Binding="{Binding Signal.DelayAlarmGrade}" Value="LowDanger">
                <Setter TargetName="AMSText" Property="Foreground" Value="Red" />
            </DataTrigger>
        </DataTemplate.Triggers>
    </DataTemplate>   

